{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/en/in-page.html"
    },
    {
      "src": "/en",
      "dest": "/en/in-page.html"
    },
    {
      "src": "/es",
      "dest": "/es/in-page.html"
    },
    {
      "src": "/pt",
      "dest": "/pt/in-page.html"
    },
    {
      "src": "/tr",
      "dest": "/tr/in-page.html"
    }
  ]
}
